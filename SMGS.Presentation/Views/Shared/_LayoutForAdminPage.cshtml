<!DOCTYPE html>
<html>
<head>
    <meta charset="utf-8" />
    <meta http-equiv="X-UA-Compatible" content="IE=edge,chrome=1" />
    <title>Dashboard</title>
    <meta content='width=device-width, initial-scale=1.0, maximum-scale=1.0, user-scalable=0' name='viewport' />
    <meta name="viewport" content="width=device-width" />
    <link href="https://maxcdn.bootstrapcdn.com/bootstrap/3.3.5/css/bootstrap.min.css" rel="stylesheet" />

    <link href="http://cdn.rawgit.com/Eonasdan/bootstrap-datetimepicker/a549aa8780dbda16f6cff545aeabc3d71073911e/build/css/bootstrap-datetimepicker.css" rel="stylesheet" />
    <link rel="apple-touch-icon" sizes="76x76" href="../assets/img/apple-icon.png" />
    <link rel="icon" type="image/png" href="../assets/img/favicon.png" />
    <meta name="viewport" content="width=device-width" />
    <!--  Bootstrap core CSS    -->
    <link href="~/Contents/css/bootstrap.min.css?v=1" rel="stylesheet" />
    <link href="~/Contents/css/bootstrap-datetimepicker.min.css" rel="stylesheet" />
    <!--  Materia. Dashboard CSS    -->
    <link href="~/Contents/css/material-dashboard.css?v=1" rel="stylesheet" />
    <!--  CSS for Demo Purpose, don't include it in your project     -->
    <link href="~/Contents/css/demo.css" rel="stylesheet" />
    <!--     Fonts and icons     -->
    <link href="http://maxcdn.bootstrapcdn.com/font-awesome/latest/css/font-awesome.min.css" rel="stylesheet">
    <link href='http://fonts.googleapis.com/css?family=Roboto:400,700,300|Material+Icons' rel='stylesheet' type='text/css'>

</head>
<body class="wrapper-admin">
    <div class="wrapper">
        <div class="nav-left">
            <div class="in-nav-left">
                <div class="sidebar" data-color="green" data-image="~/Contents/img/sidebar-1.jpg">
                    <!--
                            Tip 1: You can change the color of the sidebar using: data-color="purple | blue | green | orange | red"
                            Tip 2: you can also add an image using data-image tag
                    -->
                    <div class="logo">
                        <a href="http://fb.com/hoangphat2107" target="_blank" class="simple-text">Hoang Phat Pham</a>
                    </div>
                    <div class="sidebar-wrapper">
                        <ul class="nav">
                            <li id="li-for-dashboard">
                                <a href=".">
                                    <i class="material-icons">dashboard</i>
                                    <p>Dashboard</p>
                                </a>
                            </li>
                            <li id="li-for-booking">
                                <a href="./bookings">
                                    <i class="material-icons">schedule</i>
                                    <p>Booking</p>
                                </a>
                            </li>
                            <li id="li-for-customer">
                                <a href="./customers">
                                    <i class="material-icons">people_outline</i>
                                    <p>Customers</p>
                                </a>
                            </li>
                            <li id="li-for-employees">
                                <a href="./employees">
                                    <i class="material-icons">people_outline</i>
                                    <p>Employees</p>
                                </a>
                            </li>
                            <li id="li-for-salary">
                                <a href="./salaries">
                                    <i class="material-icons">attach_money</i>
                                    <p>Salary</p>
                                </a>
                            </li>
                            <li id="li-for-servicesadmin">
                                <a href="./adminservices">
                                    <i class="material-icons">hot_tub</i>
                                    <p>Services</p>
                                </a>
                            </li>
                            <li id="li-for-bed">
                                <a href="./beds">
                                    <i class="material-icons">weekend</i>
                                    <p>Beds</p>
                                </a>
                            </li>
                            <li id="li-for-notifications">
                                <a href="./notifications">
                                    <i class="material-icons text-gray">notifications</i>
                                    <p>Notifications</p>
                                </a>
                            </li>
                        </ul>
                    </div>
                </div>
            </div>
        </div>

        <div class="main-panel">
            <nav class="navbar navbar-transparent navbar-absolute">
                <div class="container-fluid">
                    <div class="navbar-header">
                        <button type="button" class="navbar-toggle" data-toggle="collapse">
                            <span class="sr-only">Toggle navigation</span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                            <span class="icon-bar"></span>
                        </button>
                        <a class="navbar-brand" href="">@ViewBag.Title</a>
                    </div>
                    <div class="collapse navbar-collapse">
                        <ul class="nav navbar-nav navbar-right">
                            <li>
                                <a id="notifier-btn" class="dropdown-toggle" data-toggle="dropdown">
                                    <i class="material-icons">dashboard</i>
                                    <p class="hidden-lg hidden-md">Dashboard</p>
                                </a>
                            </li>
                            <li class="dropdown">
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <i class="material-icons">notifications</i>
                                    <span id="notification-count" class="notification"></span>
                                    <p class="hidden-lg hidden-md">Notifications</p>
                                </a>
                                <ul class="dropdown-menu">
                                    <li>
                                        <a href="#">Mike John responded to your email</a>
                                    </li>
                                    <li>
                                        <a href="#">You have 5 new tasks</a>
                                    </li>
                                    <li>
                                        <a href="#">You're now friend with Andrew</a>
                                    </li>
                                    <li>
                                        <a href="#">Another Notification</a>
                                    </li>
                                    <li>
                                        <a href="./notifications">All Notifications</a>
                                    </li>
                                </ul>
                            </li>
                            <li>
                                <a href="#" class="dropdown-toggle" data-toggle="dropdown" aria-expanded="false">
                                    <i class="material-icons">person</i>
                                    <p class="hidden-lg hidden-md">Profile</p>
                                    <div class="ripple-container"></div>
                                </a>
                            </li>
                        </ul>
                        <form class="navbar-form navbar-right">
                            <div class="form-group is-empty">
                                <input id="txt-search" type="text" class="form-control label-placeholder" placeholder="Search" />
                                <span class="material-input"></span>
                                <span class="material-input"></span>
                            </div>
                            <button id="btn-search" type="button" class="btn btn-white btn-round btn-just-icon">
                                <i class="material-icons">search</i>
                                <div class="ripple-container"></div>
                            </button>
                        </form>
                    </div>
                </div>
            </nav>
            <!--Child area-->
            <div class="content">
                <div class="container-fluid">
                    @RenderBody()
                </div>
            </div>

        </div>
    </div>
    <div id="loading">
    </div>
</body>
<!--   Core JS Files   -->
<script src="~/Scripts/jquery-3.3.1.js" type="text/javascript"></script>
<script src="~/Scripts/jquery-3.3.1.min.js" type="text/javascript"></script>
<script src="~/Scripts/material.min.js" type="text/javascript"></script>
<script type="text/javascript" src="~/Scripts/moment.min.js" />
<script type="text/javascript" src="~/Scripts/Moment.js" />
<script type="text/javascript" src="~/Scripts/Moment-with-locales.min.js" />
<script type="text/javascript" src="~/Scripts/Moment-with-locales.js" />
<script type="text/javascript" src="~/Scripts/bootstrap-datetimepicker.js" />
<script type="text/javascript" src="~/Scripts/bootstrap-datetimepicker.min.js" />

<!--  Charts Plugin -->
<script src="~/Scripts/chartist.min.js"></script>
<!--  Dynamic Elements plugin -->
<script src="~/Scripts/arrive.min.js"></script>
<!--  PerfectScrollbar Library -->
<script src="~/Scripts/perfect-scrollbar.jquery.min.js"></script>
<!--  Notifications Plugin    -->
<script src="~/Scripts/bootstrap-notify.js"></script>
<!-- Material Dashboard javascript methods -->
<script src="~/Scripts/material-dashboard.js?v=1.2.4"></script>
<!-- Material Dashboard DEMO methods, don't include it in your project! -->
<script src="~/Scripts/demo.js"></script>
<!--SignalR-->
<script src="Scripts/jquery.signalR-2.2.2.min.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrap-datetimepicker.min.js" type="text/javascript"></script>
<script src="~/notification.js" type="text/javascript"></script>
<script src="~/Scripts/bootstrap.js" type="text/javascript"></script>
<script src="http://cdn.rawgit.com/Eonasdan/bootstrap-datetimepicker/a549aa8780dbda16f6cff545aeabc3d71073911e/src/js/bootstrap-datetimepicker.js"></script>

</html>
    